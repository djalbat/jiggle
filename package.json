{
  "name": "xgl",
  "author": "James Smith",
  "version": "1.6.0",
  "license": "MIT, Anti-996",
  "homepage": "https://github.com/djalbat/xgl",
  "description": "Makes use of JSX to leverage WebGL.",
  "repository": {
    "type": "git",
    "url": "https://github.com/djalbat/xgl"
  },
  "dependencies": {
    "express": "^4.16.4",
    "necessary": "^7.1.10",
    "xgl-server": "^2.0.13"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.8.3",
    "@babel/preset-env": "^7.12.7",
    "@swc/core": "^1.2.52",
    "browserify": "^16.5.0",
    "esbuild": "^0.11.8",
    "lively-cli": "^2.0.9",
    "watchful-cli": "^1.6.15"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "watchful": "watchful --transpiler=swc --bundler=esbuild --source-directory=./src --lib-directory=./lib --entry-file=example.js --bundle-file=./public/lib/example.js --pause=100",
    "batch": "npm run watchful batch --",
    "batch-debug": "npm run watchful batch -- --debug",
    "incremental": "npm run watchful incremental --",
    "incremental-debug": "npm run watchful incremental -- --debug",
    "build": "npm run clean && npm run batch",
    "build-debug": "npm run clean && npm run batch-debug",
    "watch": "npm run clean && npm run batch && npm run incremental",
    "watch-debug": "npm run clean && npm run batch-debug && npm run incremental-debug"
  },
  "main": "./lib/index.js"
}
